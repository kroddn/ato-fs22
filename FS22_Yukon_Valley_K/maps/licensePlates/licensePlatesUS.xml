<licensePlates xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../shared/xml/schema/mapLicensePlates.xsd">
    <font name="LICENSE_PLATE_US" />

    <licensePlate filename="maps/licensePlates/licensePlatesUS.i3d" node="0" type="SQUARISH" width="0.3" height="0.15">
        <font size="0.07" scaleX="0.8" scaleY="1"/>

        <variations>
            <variation> <!-- fantasy license plate -->
                <value node="0|0" posX="-0.030" numerical="true" alphabetical="true" special="true"/>
                <value node="0|1" posX="0.000" numerical="true" alphabetical="true" special="true"/>
                <value node="0|2" posX="0.030" numerical="true" alphabetical="true" special="true"/>
                <value node="0|3" posX="0.061" numerical="true" alphabetical="true" special="true"/>
                <value node="0|4" posX="0.092"  numerical="true" alphabetical="true" special="true"/>
                <value node="0|5" posX="0.123"  numerical="true" alphabetical="true" special="true"/>
            </variation>
        </variations>

        <frame node="2" widthOffset="0.005" heightOffsetTop="0.005" heightOffsetBot="0.015"/>
    </licensePlate>

    <colorConfigurations shaderParameter="colorMat4" useDefaultColors="true">
        <colorConfiguration name="$l10n_ui_colorBlue" color="0.0075 0.0212 0.1845"/>
    </colorConfigurations>

    <placement defaultType="none"/>
</licensePlates>
